<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/layout_fragment2nd_ViewGroup_textView_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="date"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/layout_fragment2nd_ViewGroup_imageView_selectedImage"
        android:layout_width="350dp"
        android:layout_height="400dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_fragment2nd_ViewGroup_textView_placeName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_fragment2nd_ViewGroup_textView_picturesNametextView"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/layout_fragment2nd_ViewGroup_textView_picturesNametextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="170dp"
        android:text="name"
        android:textColor="#E36868"
        app:layout_constraintEnd_toEndOf="@+id/layout_fragment2nd_ViewGroup_imageView_selectedImage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/layout_fragment2nd_ViewGroup_textView_placeName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:text="place"
        app:layout_constraintBottom_toTopOf="@+id/layout_fragment2nd_ViewGroup_textView_date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>